{
  "name": "peptideprotocols-ai",
  "version": "1.0.0",
  "description": "AI-Powered Peptide Therapy Platform",
  "main": "backend/server.py",
  "scripts": {
    "start": "concurrently \"yarn start:backend\" \"yarn start:frontend\"",
    "start:backend": "cd backend && python server.py",
    "start:frontend": "cd frontend && serve -s build -l 3000",
    "build": "cd frontend && yarn build",
    "install:all": "yarn install && cd frontend && yarn install && cd ../backend && pip install -r requirements.txt",
    "deploy": "yarn build && pm2 restart ecosystem.config.js"
  },
  "keywords": ["peptide", "therapy", "medical", "ai", "protocols"],
  "author": "PeptideProtocols.ai",
  "license": "Proprietary",
  "dependencies": {
    "concurrently": "^8.2.0",
    "pm2": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}